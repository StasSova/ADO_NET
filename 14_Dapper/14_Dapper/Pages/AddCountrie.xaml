<Window x:Class="_14_Dapper.Pages.AddCountrie"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_14_Dapper.Pages"
        mc:Ignorable="d"
        Title="AddCountrie" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Text="Название страны:"/>
            <TextBox x:Name="CountryNameTextBox" Margin="0 5"/>
            <Button Content="Сохранить" Click="SaveCountry_Click" Margin="0 10"/>
        </StackPanel>
    </Grid>
</Window>
